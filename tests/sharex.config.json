{
  "Version": "13.6.1",
  "Name": "Muploader",
  "DestinationType": "ImageUploader",
  "RequestMethod": "POST",
  "RequestURL": "https://your-domain.sk/api/uploadFile",
  "Headers": {
    "Authorization": "authorization if you want"
  },
  "Body": "MultipartFormData",
  "Arguments": {
    "gcaptcha": "none",
    "tos-accept": "true",
    "withoutAuth": "true"
  },
  "FileFormName": "file",
  "URL": "$json:message.url$"
}